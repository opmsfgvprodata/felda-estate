                     console.log(btnClick_Kesukaran);
                     console.log(HargaKesukaranlist.length);
                     if (HargaKesukaranlist.length > 0) {
                         var addlist_Kesukaran = document.createElement('div');
                         var addlist_TahapKesukaran = document.createElement('div');
