                        }
                        if (ClosingStatus =='True') {
                            $(".postSAPBtn").prop("disabled", false);
                            $(".verifyBtn").prop("disabled", false);
                        }
                        else {
                            $(".postSAPBtn").prop("disabled", true);
                            $(".verifyBtn").prop("disabled", false);
                        }
                        debugger
                        $("#divverify").show();
                    //end
                    }
                });
            });

