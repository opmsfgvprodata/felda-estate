                console.log($(this).val());
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("generateRefNo", "BizTransac")',
                    dataType: 'json',
                    contentType: 'application/json',
                    async: false,
                    data: JSON.stringify({ docType: $(".docTypeDR").val(), SAPPostRefNoID: $(".idDR").val() }),
