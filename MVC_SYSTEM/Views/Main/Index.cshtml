                                }
                            }
                        },
                        plotOptions: {
                            radialBar: {
                                dataLabels: {
                                    total: {
                                        show: true,
                                        label: 'TOTAL'
                                    }
                                }
                            }
                        },
                        labels: dataLabelLadang
                    };

                    var chart = new ApexCharts(document.querySelector("#akaun"), options);
                    chart.render();
                },
            });

        };
        barchartaccount();

