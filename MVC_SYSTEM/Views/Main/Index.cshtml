                            events: {
                                dataPointSelection: (event, chartContext, config) => {
                                    datatableKerakyatan('Pekerja mengikut kerakyatan ' + config.w.config.labels[config.dataPointIndex], config.w.config.labels[config.dataPointIndex]);
                                }
                            }
                        },
                        responsive: [{
                            breakpoint: 480,
                            options: {
                                chart: {
                                    width: 200
                                },
                                legend: {
                                    position: 'bottom'
                                }
                            }
                        }],
                        plotOptions: {
                            pie: {
                                donut: {
                                    labels: {
                                        show: true,
                                        name: {
                                            show: true,
                                            fontSize: '22px',
                                            fontFamily: 'Rubik',
                                            color: '#dfsda',
                                            offsetY: -10
                                        },
                                        value: {
                                            show: true,
                                            fontSize: '30px',
                                            fontFamily: 'Helvetica, Arial, sans-serif',
                                            color: undefined,
                                            offsetY: 16,
                                            formatter: function (val) {
                                                return val
                                            }
                                        },
                                        total: {
                                            show: true,
                                            label: 'Total',
                                            color: '#373d3f',
                                            formatter: function (w) {
                                                return w.globals.seriesTotals.reduce((a, b) => {
                                                    return a + b
                                                }, 0)
                                            }
                                        }
                                    }
                                }
                            }
                        },
                    };

                    var chart = new ApexCharts(document.querySelector("#chartkerakyatan"), options);
                    chart.render();
                },
            });

        };
        kerakyatanchart();

